import java.io.*;
public class GCD
{
    public static void main(String[] args)
    {
        int a= 36, b=60;
        int gcd= findGCD(a,b);
        System.out.println("GCD of"+a+"and"+b+"is:"+gcd);
    }
    public static int findGCD(int a,int b)
    {
        while (b!=0)
        {
            int temp =b;
            b= a % b ;
            a= temp;
        }
        return a;
    }
}
