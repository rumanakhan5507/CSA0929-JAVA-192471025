import java.io.*;
public class DecimaltoBinary
{
    public static void main(String[] args)
{
    int decimal = 25;
    String binary = "";
    while (decimal>0)
    {
        int remainder = decimal%2;
        binary= remainder + binary;
        decimal = decimal/2;
    }
    System.out.println("Binary:"+binary);
    }
}
OUTPUT: 
Binary:11001
