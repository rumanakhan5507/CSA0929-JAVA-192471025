import java.io.*;
class VotingEligibility
{
    public static void main(String[] args)
    {
        int age = 20;
        if (age>=18)
        {
            System.out.println("You are eligible to vote");
        }
        else
        {
            System.out.println("You are not eligible to vote");
        }
        }
    }
OUTPUT:
You are eligible to vote
