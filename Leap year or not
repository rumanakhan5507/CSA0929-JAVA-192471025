import java.io.*;
class LeapYearCheck 
{
    public static void main(String[] args)
    {
        int year= 2025;
        if (year%4%400%100==0)
        {
            System.out.println(year+ "is a leap year");
        } 
        else
        {
            System.out.println(year+ "is not a leap year");
        }
        }
    }
OUTPUT:
2025 is not a leap year
