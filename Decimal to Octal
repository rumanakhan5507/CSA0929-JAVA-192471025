import java.io.*;
public class DecimaltoOctal
{ 
    public static void main(String[] args)
    {
        int decimal= 25;
        String octal= "";
        while (decimal>0)
        {
            int remainder = decimal % 8;
            octal= remainder + octal;
            decimal = decimal/8;
        }
        System.out.println("Octal:"+octal);
    }
}
OUTPUT: 
Octal:31
    
